

//brute approach

#include<iostream>
using namespace std;

#include<math.h>
 

int findDuplicate(int *arr, int n)
{ int c=0;
    //Write your code here
    for(int i=0;i<n;i++)
    {
        for(int j=0;j<n;j++)
        {
            if (arr[i]==arr[j])
               { c=j;
                }
        }
    }
 return arr[c];
}
int main()
{
    int arr[]={ 1,2,3,4,4,9};
    int n=sizeof(arr)/sizeof(int);
     
   cout<<findDuplicate(arr,n);
   

 }  
