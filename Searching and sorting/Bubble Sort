#include<iostream>
using namespace std;
void bubbleSort(int arr[],int n)
{
    for(int j=0;j<n-1;j++)
    {


    for(int i=0;i<n-1-j;i++)

    {
        if(arr[i]>arr[i+1])
           {
           //swap
       int temp=arr[i];
       arr[i]==arr[i+1];
       arr[i+1]=temp;
           }
    }

    }
}

int main()
{

    int arr[100] ;
    int n;
    cout<<"enter the number of element";
    cin>>n;
    cout<<"enter the array element";

   //input lo
   for(int i=0;i<n;i++)
   {

       cin>>arr[i];
   }
   //function call
    bubbleSort(arr,n);
    //print
    for(int i=0;i<n;i++)

{

    cout<<arr[i] <<"";

}
cout<<endl;
}
